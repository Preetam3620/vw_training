<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
                           http://www.springframework.org/schema/context
                           http://www.springframework.org/schema/context/spring-context-3.2.xsd">



     <bean id="accountbean" class="propertyinjection.Account">
        <property name="acctid"  value="10">
        </property>
        <property name="acctName"  value="namrata"></property>
        <property name="acctBal"  value="5000"></property>
     </bean>
     
     <bean id="empbean" class="propertyinjection.Employee">
        <property name="empid"  value="1">
        </property>
        <property name="empname"  value="namrata"></property>
        <property name="empact"  ref="accountbean"></property>
     </bean>


</beans>